    Report.PrintOut False
End Sub

Private Sub CmdOption_Click()
    Report.PrinterSetup Me.hWnd
    CRViewer1.Refresh
End Sub

Private Sub Form_Load()
    Dim p As Printer
    cboPrinter.Clear
    For Each p In Printers
        cboPrinter.AddItem p.DeviceName
    Next
    cboPrinter.Text = GetTxt("Setting.ini", "Printer", "LaporanPasienPulang")
End Sub

Private Sub Form_Resize()
    CRViewer1.Top = 0
    CRViewer1.Left = 0
    CRViewer1.Height = ScaleHeight
    CRViewer1.Width = ScaleWidth
End Sub

Private Sub Form_Unload(Cancel As Integer)

    Set frmCRLampiranBeritaAcaraKasBank = Nothing
End Sub

Public Sub Cetak(idTemp As String, view As String)
On Error GoTo errLoad

Set frmCRLampiranBeritaAcaraKasBank = Nothing
Dim adocmd As New ADODB.Command
Set Report = New cr_LampiranBeritaAcaraKasBank

    strSQL = "select * from temp_lampiranba_kasbank_t " & _
            "where idtemplampiran = '" & idTemp & "'"
            
    adocmd.CommandText = strSQL
    adocmd.CommandType = adCmdText
        
    With Report
        .database.AddADOCommand CN_String, adocmd

            .un100k.SetUnboundFieldSource ("{ado.pecahan100k}")
            .un50k.SetUnboundFieldSource ("{ado.pecahan50k}")
            .un20k.SetUnboundFieldSource ("{ado.pecahan20k}")
            .un10k.SetUnboundFieldSource ("{ado.pecahan10k}")
            .un5k.SetUnboundFieldSource ("{ado.pecahan5k}")
            .un2k.SetUnboundFieldSource ("{ado.pecahan2k}")
            .un1k.SetUnboundFieldSource ("{ado.pecahan1k}")
            .un500.SetUnboundFieldSource ("{ado.pecahan500}")
            .un200.SetUnboundFieldSource ("{ado.pecahan200}")
            .un100.SetUnboundFieldSource ("{ado.pecahan100}")
            
            .ucBriBanks.SetUnboundFieldSource ("{ado.bribank}")
            .ucDepositoBanks.SetUnboundFieldSource ("{ado.depositobank}")
            .usTerbilangs.SetUnboundFieldSource ("{ado.terbilangbank}")
            
            If view = "false" Then
                Dim strPrinter As String
'
                strPrinter = GetTxt("Setting.ini", "Printer", "LaporanPasienPulang")
                .SelectPrinter "winspool", strPrinter, "Ne00:"
                .PrintOut False
                Unload Me
            Else
                With CRViewer1
                    .ReportSource = Report
                    .ViewReport
                    .Zoom 1
                End With
                Me.Show
            End If
        'End If
    End With
Exit Sub
errLoad:
End Sub
